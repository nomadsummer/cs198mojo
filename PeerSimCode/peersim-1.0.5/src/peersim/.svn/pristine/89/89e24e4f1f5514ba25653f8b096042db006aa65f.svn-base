package peersim.ps.pubsub;

import java.math.BigInteger;

/**
 * @author Fatemeh Rahimian	
 */
public final class PublishEvent {
	private final BigInteger pubsubPeerId;
	private final BigInteger topicId;

//------------------------------------------------------------------------	
	public PublishEvent(BigInteger pubsubPeerId, BigInteger topicId) {
		this.pubsubPeerId = pubsubPeerId;
		this.topicId = topicId;
	}

//------------------------------------------------------------------------	
	public BigInteger getPubSubPeerId() {
		return this.pubsubPeerId;
	}

//------------------------------------------------------------------------	
	public BigInteger getTopicId() {
		return topicId;
	}
}
